expression -> term (("+" | "-") term)*

term -> factor (("*" | "/") factor)*

factor -> number | identifier | "(" expression ")" | "-" factor

number = digit+

identifier = letter (letter | digit)*

digit -> "1" | "2" | "3" | "4" | "5" | "6" | "7" | "8" | "9" | "0"

letter -> "a" | "b" ... "z" | "A" | "B" ... "Z"
